{
    "name": "trendyminds/janitor",
    "description": "Make block organization and presentation a snap!",
    "keywords": [
        "statamic",
        "blocks",
        "dev"
    ],
    "version": "1.1.1",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Trendyminds\\Janitor\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Trendyminds\\Janitor\\Tests\\": "tests"
        }
    },
    "require": {
        "statamic/cms": "^6.0",
        "spatie/browsershot": "^5.2"
    },
    "config": {
        "allow-plugins": {
            "pixelfear/composer-dist-plugin": true
        }
    },
    "extra": {
        "statamic": {
            "name": "Janitor",
            "description": "Make block organization and presentation a snap!"
        },
        "laravel": {
            "providers": [
                "Trendyminds\\Janitor\\ServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require-dev": {
        "laravel/pint": "^1.27"
    }
}
